<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.module_demo_0518">

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 权限设置：intent-filter标签(最高优先级) -->
    <!-- 注意：标签activity中的names属性中的“.”与“不带.”直接带上包名 -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_pat"
        android:label="净界"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".DoorActivity" />
        <activity android:name=".SelectiveSensorActivity" />
        <activity android:name=".TempAndHumidityActivity" />
        <activity android:name=".Remote_Activity" />
        <activity android:name=".Socket_many_Activity" />
        <activity android:name="Snack.MainSnackActivity" />
        <activity android:name=".ColoredEggsActivity" />
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".ConnectUsActivity"></activity>
    </application>

</manifest>